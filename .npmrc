# Ensure consistent builds across local and Vercel
legacy-peer-deps=true
strict-peer-dependencies=false
engine-strict=false

# Improve build performance
fetch-timeout=120000
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000

# Disable npm audit during install (speeds up build)
audit=false

# Use exact versions to avoid surprises
save-exact=false
